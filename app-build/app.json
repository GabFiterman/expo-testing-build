{
    "expo": {
        "name": "app-build",
        "slug": "app-build",
        "version": "1.2.1",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "userInterfaceStyle": "light",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "plugins": [
            [
                "expo-location",
                {
                    "locationAlwaysAndWhenInUsePermission": "Podes permitir que $(PRODUCT_NAME) use sua localização?."
                }
            ]
        ],
        "ios": {
            "buildNumber": "1.0.0",
            "supportsTablet": true,
            "bundleIdentifier": "com.mycompany.myapp",
            "infoPlist": {
                "NSLocationWhenInUseUsageDescription": "Este aplicativo utiliza sua localização para fornecer um melhor serviço.",
                "NSLocationAlwaysUsageDescription": "Este aplicativo precisa de acesso à sua localização o tempo todo para funcionar corretamente."
            }
        },
        "android": {
            "versionCode": 1,
            "adaptiveIcon": {
                "foregroundImage": "./assets/adaptive-icon.png",
                "backgroundColor": "#ffffff"
            },
            "permissions": ["ACCESS_FINE_LOCATION", "ACCESS_COARSE_LOCATION"],
            "package": "com.gabfiterman.appbuild"
        },
        "web": {
            "favicon": "./assets/favicon.png"
        },
        "extra": {
            "eas": {
                "projectId": "832cc71e-41c0-41e1-9802-d053f5ea836e"
            },
            "expo.webBrowserHost": "*"
        },
        "owner": "gabfiterman"
    }
}
